.paginator {
font: 14px Arial,Helvetica,sans-serif;
color: #AAA;
margin: 20px 0;
line-height: 150%;
text-align: center;
}

.paginator .prev {
margin-right: 20px;
}


.paginator .thispage {
color: white;
background: #c0c0c0;
}


.paginator a, .thispage, .break {
padding: 0 4px;
margin: 2px;
}

.paginator .break {
color: #999;
}

.paginator .next {
margin-left: 20px;
}







